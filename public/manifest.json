{
    "manifest_version": 2,
    "name": "github create pr 'are you sure ?'",
    "version": "1.0",

    "content_scripts": [
        {
            "matches": [
                "https://github.com/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],

    "permissions": [
        "tabs"
    ],

    "web_accessible_resources": [
        "*.png"
    ]
}
